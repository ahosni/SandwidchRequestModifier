{
  "manifest_version": 2,
  "name": "Sandwidch Request Modifier 三文治请求修改器",
  "short_name": "Sandwidch",
  "version": "2.2.3",
  "description": "A super handy chrome extension for modifying headers, URL parameters, user agents and etc., with cases and domain keywords support.",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'",
  "icons": {
    "16": "img/logo.png",
    "48": "img/logo.png",
    "128": "img/logo.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "bookmarks",
    "contextMenus",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "omnibox": {
    "keyword": "Sandwidch"
  },
  "options_page": "html/main.html",
  "devtools_page": "html/devtools.html",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_popup": "html/popup.html"
  },
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "*://*/*"
      ],
      "js": [
        "js/content_script.js"
      ]
    }
  ]
}