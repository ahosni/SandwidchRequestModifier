{
  "manifest_version": 2,
  "name": "Sailfish Requester 旗鱼 请求管理器",
  "short_name": "Sailfish",
  "version": "1.0.1",
  "description": "Customizing requests for modifying headers, user agents, url parameters and etc.",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'",
  "icons": {
    "16": "img/logo.png",
    "48": "img/logo.png",
    "128": "img/logo.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "bookmarks",
    "contextMenus",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "omnibox": {
    "keyword": "Sailfish"
  },
  "options_page": "html/main.html",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
  }
}